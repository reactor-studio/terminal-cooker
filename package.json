{
	"name": "terminal-cooker",
	"version": "0.0.0",
	"description": "Cook inside your terminal",
	"license": "MIT",
	"repository": "reactor-studio/terminal-cooker",
	"author": {
		"name": "Dino Trojak",
		"email": "dinotrojak@gmail.com",
		"url": "dinodsaur.us"
	},
	"bin": "cli.js",
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"test": "xo && ava"
	},
	"files": [
		"cli.js",
		"ui.js"
	],
	"keywords": [
		"terminal",
		"cooking"
	],
	"dependencies": {
		"home-dir": "^1.0.0",
		"import-jsx": "^1.3.0",
		"ink": "^0.5.0",
		"ink-spinner": "^2.0.0",
		"meow": "^5.0.0",
		"prop-types": "^15.6.1"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"eslint-config-xo-react": "^0.16.0",
		"eslint-plugin-react": "^7.7.0",
		"xo": "0.20.3"
	},
	"ava": {
		"babel": {
			"presets": [
				"@ava/stage-4"
			],
			"plugins": [
				[
					"transform-react-jsx",
					{
						"pragma": "h",
						"useBuiltIns": true
					}
				]
			]
		}
	},
	"xo": {
		"extends": [
			"xo-react"
		],
		"rules": {
			"react/no-unused-prop-types": 1
		},
		"settings": {
			"react": {
				"pragma": "h"
			}
		}
	}
}
